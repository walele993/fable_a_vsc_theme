  "tokenColors": [
    {
      "name": "Functions/Methods",
      "scope": [
        "entity.name.function",
        "meta.method"
      ],
      "settings": {
        "foreground": "#D6409F",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variables/Attributes",
      "scope": [
        "variable",
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#8A9B6E",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#A3C9A3",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords/Types",
      "scope": [
        "keyword",
        "storage.type",
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#D6409F",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Numbers/Booleans",
      "scope": [
        "constant.numeric",
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#B8A898"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#D6409F"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "#A0988C",
        "fontStyle": "italic"
      }
    },
    {
      "name": "HTML Tags",
      "scope": [
        "entity.name.tag.html"
      ],
      "settings": {
        "foreground": "#D6409F"
      }
    },
    {
      "name": "Types/Classes",
      "scope": [
        "storage.type",
        "support.type",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#7A4EBF"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "entity.other.attribute-name.html"
      ],
      "settings": {
        "foreground": "#8A9B6E"
      }
    },
    {
      "name": "CSS Selectors",
      "scope": [
        "entity.name.tag.css"
      ],
      "settings": {
        "foreground": "#5E8A5E"
      }
    },
    {
      "name": "CSS Properties",
      "scope": [
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#D6409F"
      }
    }
  ]
}
