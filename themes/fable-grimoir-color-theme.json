{
  "name": "Grimoire",
  "type": "dark",
  "colors": {
    "editor.background": "#1E1E1E",
    "editor.foreground": "#D4D4D4",
    "statusBar.background": "#2D2D2D",
    "statusBar.foreground": "#FF7EB6",
    "sideBar.background": "#262626",
    "sideBar.foreground": "#757575",
    "sideBarSectionHeader.background": "#1E1E1E",  
    "sideBarSectionHeader.foreground": "#757575",
    "list.hoverForeground": "#FF7EB6",
    "activityBar.background": "#262626", 
    "activityBar.foreground": "#FF7EB6",
    "activityBar.inactiveForeground": "#757575",
    "activityBarBadge.background": "#FF7EB6",
    "activityBarBadge.foreground": "#1E1E1E",
    "list.activeSelectionForeground": "#FF7EB6",
    "list.activeSelectionBackground": "#3A3A3A",
    "list.inactiveSelectionForeground": "#FF7EB6",
    "list.inactiveSelectionBackground": "#2D2D2D",
    "menu.selectionForeground": "#FF7EB6",
    "menu.selectionBackground": "#3A3A3A",
    "button.background": "#D16D9C",
    "editorCursor.foreground": "#9E7EBE",
    "editorLineNumber.foreground": "#5F6D5F",
    "editor.selectionBackground": "#3A3A3A",
    "editor.selectionHighlightBackground": "#FF7EB630",
    "focusBorder": "#8A9B6E"
  },
  "tokenColors": [
    {
      "name": "Functions/Methods",
      "scope": ["entity.name.function", "meta.method"],
      "settings": {
        "foreground": "#FF7EB6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable"],
      "settings": {
        "foreground": "#8A9B6E",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Strings",
      "scope": ["string"],
      "settings": {
        "foreground": "#A3C9A3",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#FF7EB6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#C49B6C"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment"],
      "settings": {
        "foreground": "#666666",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Types/Classes",
      "scope": ["storage.type", "support.type", "entity.name.class"],
      "settings": {
        "foreground": "#9E7EBE"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#D16D9C"
      }
    },
    {
      "name": "HTML Tags",
      "scope": ["entity.name.tag.html"],
      "settings": {
        "foreground": "#FF7EB6"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": ["entity.other.attribute-name.html"],
      "settings": {
        "foreground": "#8A9B6E"
      }
    },
    {
      "name": "CSS Selectors",
      "scope": ["entity.name.tag.css"],
      "settings": {
        "foreground": "#8A9B6E"
      }
    },
    {
      "name": "CSS Properties",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#D16D9C"
      }
    }
  ]
}