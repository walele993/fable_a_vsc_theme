  "titleBar.border": "#1C1C1C",
  "editorGroupHeader.tabsBackground": "#1C1C1C"
  },
  "tokenColors": [
    {
      "name": "Functions/Methods",
      "scope": ["entity.name.function", "meta.method"],
      "settings": {
        "foreground": "#FF7EB6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable"],
      "settings": {
        "foreground": "#8A9B6E",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Strings",
      "scope": ["string"],
      "settings": {
        "foreground": "#A3C9A3",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#FF7EB6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#C49B6C"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment"],
      "settings": {
        "foreground": "#666666",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Types/Classes",
      "scope": ["storage.type", "support.type", "entity.name.class"],
      "settings": {
        "foreground": "#9E7EBE"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#D16D9C"
      }
    },
    {
      "name": "HTML Tags",
      "scope": ["entity.name.tag.html"],
      "settings": {
        "foreground": "#FF7EB6"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": ["entity.other.attribute-name.html"],
      "settings": {
        "foreground": "#8A9B6E"
      }
    },
    {
      "name": "CSS Selectors",
      "scope": ["entity.name.tag.css"],
      "settings": {
        "foreground": "#8A9B6E"
      }
    },
    {
      "name": "CSS Properties",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#D16D9C"
      }
    }
  ]
}